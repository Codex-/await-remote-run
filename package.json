{
  "name": "await-remote-action",
  "version": "0.0.0",
  "private": true,
  "description": "Await the result of a remote repository action.",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "format:check": "prettier --check **/*.ts",
    "format:write": "npm run format:check -- --write",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "style": "npm run lint && npm run format:check",
    "test": "jest",
    "all": "npm run build && npm run style && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Codex-/await-remote-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Alex Miller",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.2",
    "@typescript-eslint/parser": "^4.28.3",
    "@vercel/ncc": "^0.28.6",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.0.6",
    "jest-circus": "^27.0.6",
    "js-yaml": "^4.1.0",
    "prettier": "2.3.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  }
}
